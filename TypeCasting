public class TypeCastingDemo {

	public static void main(String[] args) {
		// widening / implicit type casting
		byte b=10;//1 byte
		int i=b;//2 byte
		System.out.println(i);
		
		float f=22.3f;
		double d=f;
		System.out.println(d);
		
		// narrowing/explicit type casting
		float f2=34.56f;
		int r=(int)f2;
		System.out.println(r);

	}

}
